   <children>
      <Label layoutX="415.0" layoutY="14.0" text="Ficha Tecnica" />
      <Button fx:id="btn_view_article" layoutX="34.0" layoutY="548.0" mnemonicParsing="false" onAction="#goToViewArticle" prefHeight="34.0" prefWidth="110.0" text="Ver Artigos" />
      <Button fx:id="btn_return" layoutX="838.0" layoutY="553.0" mnemonicParsing="false" onAction="#goReturn" text="Voltar" />
